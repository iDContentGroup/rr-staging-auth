{
  "name": "rr-staging-auth",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:staging",
    "start:staging": "ng serve --open --progress",
    "start:prod": "ng serve --open --progress -c=serve-production",
    "build": "npm run build:staging",
    "build:staging": "ng build --prod --aot -c=build-staging",
    "build:prod": "ng build --prod --aot -c=build-production",
    "deploy": "npm run deploy:staging",
    "deploy:staging": "firebase use staging && firebase deploy --only hosting",
    "deploy:prod": "npm run deploy:prod:full",
    "deploy:prod:full": "npm run build:prod && npm run deploy:prod:hosting",
    "deploy:prod:hosting": "firebase use prod && firebase deploy --only hosting",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@moomoomamoo/rocket-rounding-types": "^0.4.12",
    "firebase": "^7.6.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
